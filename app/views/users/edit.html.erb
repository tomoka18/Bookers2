<h2>User info</h2>
<%= form_with model:@user, local:true do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"Name"%>

  <%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.png"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.attachment_field :profile_image, placeholder: "Image" %>



  <%= f.submit "変更を保存" %>
<% end %>